
@{
    ViewBag.Title = "Home Page";
}

@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#calendar').fullCalendar({
                theme: true,
                header: {
                    left: "prev,next,today",
                    center: "title",
                    right: "month,agendWeel,agendDay"
                },
                defaultView: "agendDay",
                editable: true,
                allDaySlot: false,
                selectable: true,
                slotMinutes: 15,
                event: "/home/getevents/"
            });
        });
    </script>
}


<div class="jumbotron">
    <h1>Full Calendar Demo</h1>

</div>

<div class="row">
    <div class="col-md-12">
        <div id="calendar"></div>
    </div>
</div>